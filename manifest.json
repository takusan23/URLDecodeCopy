{
    "name": "URLDecodeCopy",
    "version": "1.2",
    "manifest_version": 2,
    "description": "URLエンコードされたURLをURLデコードしてコピーします。",
    "permissions": [
        "activeTab",
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "clipboardWrite"
    ],
    "icons":{
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}